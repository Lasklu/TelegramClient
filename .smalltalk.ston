SmalltalkCISpec {

  #preLoading : 'scripts/preLoading.st',
  #loading : [
    SCIMetacelloLoadSpec {
      #baseline : 'TelegramClient',
      #platforms : [ #squeak ],
      #directory : 'packages',
      #load : [ 'tests' ]
    }
  ],
   #testing : {
    #coverage : {
      #packages : [ 'TelegramClient-Core.*' ],
      #service: #coverall
    },
    #exclude : {
      #classes : [ #TCTLinterTests ]
    },
    #defaultTimeout : 60
  }
}
